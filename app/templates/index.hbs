<div class="{{styles.indexRoute}} {{vudu 'liquidInner'}}" id="index-route-scroll-context">
  {{#index-scroll-container indexSubSection=s}}
    {{#each model as |section|}}
      {{#way-point
        action=(action "didHitWaypoint" section.waypoint null)
        contextElementId="index-route-scroll-context"
        class=section.className }}
        {{{section.copy}}}
      {{/way-point}}
    {{/each}}
  {{/index-scroll-container}}
  <div class="{{styles.triangleContainer}} {{vudu 'footerHeight relative'}}">
    {{scroll-to-arrow direction="up" }}
    <img src='images/triangle-south.svg'>
  </div>
</div>
