<div>
  <p class={{vudu 'p col10 mxAuto py5 serifLight'}}>{{model.title}}</p>
  <img class={{vudu 'col12'}} src={{model.featuredImage.file.url}} />
  <p class={{vudu 'p col10 mxAuto py5 serifLight'}}>{{model.summary}}</p>
  <div class={{vudu 'mdFlex col12 mxAuto spaceAround wrap'}}>
    {{#each model.assets as |asset|}}
      <div class={{vudu 'col12 mdCol6 mdFlex justifyCenter alignCenter'}}>
        <img class={{vudu 'col12 mdCol6 p4'}} src={{asset.file.url}} />
      </div>
    {{/each}}
  </div>
  <p class={{vudu 'p py5 col10 mxAuto serifLight'}}>{{model.body}}</p>
  <div class={{vudu 'col10 mxAuto py5'}}>
    <p class={{vudu 'p serifLight'}}>{{model.takeaway}}</p>
    <a style={{vudu 'black'}} href={{model.url}} target="_blank">
      <p class={{vudu 'p serifLight py5 black'}}>Checkout Dame Product</p>
    </a>
  </div>
</div>
