<div>
  <p class={{vudu 'title col11 mxAuto py2_5 serifLight'}}>{{model.title}}</p>
  <img class={{vudu 'col12'}} src={{model.featuredImage.file.url}} />
  <p class={{vudu 'title col11 pt2_5 mxAuto serifLight'}}>{{model.summaryHeader}}</p>
  <p class={{vudu 'p col11 mxAuto py2_5 serifLight mdTwoColumn'}}>{{model.summary}}</p>
  <div class={{vudu 'mdFlex col11 mxAuto spaceAround wrap'}}>
    {{#each model.assets as |asset|}}
      <div class={{vudu 'col12 mdCol6 mxAuto mdFlex justifyCenter alignCenter'}}>
        <img class={{vudu 'col12 p4'}} src={{asset.file.url}} />
      </div>
    {{/each}}
  </div>
  <p class={{vudu 'title col11 pt2_5 mxAuto serifLight'}}>{{model.bodyHeader}}</p>
  <p class={{vudu 'p py2_5 col11 mxAuto serifLight mdTwoColumn'}}>{{model.body}}</p>
  <div class={{vudu 'col11 mxAuto py2_5'}}>
    <p class={{vudu 'p serifLight col12 mdCol6'}}>{{model.takeaway}}</p>
    <a class={{vudu 'linkStyle inlineBlock'}} href={{model.url}} target="_blank">
      <p class={{vudu 'title serifLight py2_5'}}>Checkout Dame Product</p>
    </a>
  </div>
</div>
