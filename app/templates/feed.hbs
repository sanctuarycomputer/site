
{{#scroll-route contentLoaded=sanctu.feedData}}
  <div class="{{styles.feedRoute}} {{vudu 'liquidInner'}}">
    <div class={{vudu 'relative'}}>
      {{scroll-to-arrow direction="down"}}
      <img class={{vudu 'mirror fullWidth'}} src='images/stroke.svg'>
    </div>
    <div class={{styles.feedWrapper}}>
      {{#if sanctu.feedData}}
        {{outlet}}
      {{else}}
        Loading...
      {{/if}}
    </div>
  </div>
{{/scroll-route}}
